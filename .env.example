# Режим окружения
NODE_ENV=development

# Gate.io API настройки
GATE_API_KEY=your_api_key_here
GATE_SECRET_KEY=your_secret_key_here
GATE_PASSPHRASE=your_passphrase_here
GATE_SANDBOX=true

# Настройки логирования
LOG_LEVEL=info
LOG_DIR=./logs
LOG_FILE=true
LOG_ERROR_FILE=true
LOG_CONSOLE=true
LOG_COLORIZE=true
LOG_MASK_SENSITIVE=true
LOG_MAX_FILES=14
LOG_MAX_SIZE=20m
LOG_DATE_PATTERN=YYYY-MM-DD

# Терминальные настройки
TERMINAL_COLORS=true
TERMINAL_UNICODE=true

# Event Bus настройки
EVENT_BUS_MAX_LISTENERS=20

# Дополнительные настройки
SESSION_ID=
DEBUG=false